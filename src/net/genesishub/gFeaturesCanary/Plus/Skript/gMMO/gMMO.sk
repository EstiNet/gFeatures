options:
	cooldown.serratedstrikes: 2 minutes
	swords.levelup.amount: 20
on death:
	attacker is a player
	victim isn't a player
	attacker is holding a sword
	if {@Swords Ability} is true:
		set {_health} to 75
		if victim is ender dragon or wither:
			set {_health} to {_health} * {@multiplier.tier4}
		if victim is creeper or enderman:
			set {_health} to {_health} * {@multiplier.tier3}
		if victim is spider or silverfish or blaze or skeleton or ghast or zombie pigman or wither skeleton:
			set {_health} to {_health} * {@multiplier.tier2}
		if victim is magma cube or iron golem or slime or zombie or cave spider:
			set {_health} to {_health} * {@multiplier.tier1}
		else:
			set  {_health} to {_health} * {@multiplier.friendly}
		add {_health} to {skmmo::%{skmmoVar.%attacker%}%::swords::xp}
		make console execute command "skmmo check %attacker% Swords"
		#-------------------------------------------------------#
		# A.		      --BLEEDING--			#
		#-------------------------------------------------------#
on damage:
	attacker is a player
	attacker is holding a sword
	chance of {skmmo::%{skmmoVar.%attacker%}%::swords::skills::bleed::chance}%:
		send "&c**YOU'RE BLEEDING OUT**" to victim
		send "&a**%victim% IS BLEEDING OUT**" to attacker
		set {skmmo::%{skmmoVar.%victim%}%::swords::bleeding} to true
		wait {skmmo::%{skmmoVar.%attacker%}%::swords::skills::bleed::duration}
		set {skmmo::%{skmmoVar.%victim%}%::swords::bleeding} to false
		send "&a**YOU STOPPED THE BLEEDING**" to victim
		#-------------------------------------------------------#
		# B.		   --COUNTER ATTACK--			#
		#-------------------------------------------------------#
on damage:
	victim is a player
	if victim is defending:
		chance of {skmmo::%{skmmoVar.%victim%}%::swords::skills::counterattack::chance}%:
			if victim's health is more than 1:
				set {_CountAtt} to the damage
				set {_CountAtt} to {_CountAtt} / 2
				damage the attacker by {_CountAtt}
				send "&c**COUNTER ATTACK**" to attacker
				send "&a**COUNTER ATTACK**" to victim
		#-------------------------------------------------------#
		# C.		  --SERRATED STRIKES--			#
		#-------------------------------------------------------#
on right click:
	if {@Swords Ability} is true:
		player is holding a sword:
			if clicked block is iron block or gold block:
				stop
			else:
				if {skmmo::%{skmmoVar.%player%}%::swords::skills::serratedstrikes::toggle} is false:
					set {_difference} to difference between {skmmo::%{skmmoVar.%player%}%::swords::skills::serratedstrikes::cooldowner} and now
					if {_difference} is less than {@cooldown.serratedstrikes}:
						set {_difference} to difference between {@cooldown.serratedstrikes} and {_difference}
						send "&cYou are too tired to use this ability. &e(%{_difference}%)"
						if {skmmo::%{skmmoVar.%player%}%::swords::skills::serratedstrikes::cooldownmsg} is false:
							set {skmmo::%{skmmoVar.%player%}%::swords::skills::serratedstrikes::cooldownmsg} to true
							wait {_difference}
							send "&aYour &eSerrated Strikes &ais Refreshed!"
							set {skmmo::%{skmmoVar.%player%}%::swords::skills::serratedstrikes::cooldownmsg} to false
					else:
						send "&a** YOU RAISE YOUR SWORD **"
						set {skmmo::%{skmmoVar.%player%}%::swords::skills::serratedstrikes::toggle} to true
						wait 1 seconds
						if {skmmo::%{skmmoVar.%player%}%::swords::skills::serratedstrikes::use} is false:
							send "&7** YOU LOWER YOUR SWORD **"
							set {skmmo::%{skmmoVar.%player%}%::swords::skills::serratedstrikes::toggle} to false

