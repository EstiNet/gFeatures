			send "&7**YOU'VE STOPPED THE BLEEDING**" to victim
			set {skmmo::%{skmmoVar.%victim%}%::swords::bleeding} to false

every second:
	loop all players:
		if {skmmo::%{skmmoVar.%loop-player%}%::swords::bleeding} is true:
			damage loop-player by 0.5

on death of player:
	if {skmmo::%{skmmoVar.%player%}%::swords::bleeding} is true:
		set {skmmo::%{skmmoVar.%player%}%::swords::bleeding} to false
			#-----------------------------------------------------------------------#
