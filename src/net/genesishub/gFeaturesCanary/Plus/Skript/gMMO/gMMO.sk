		#-------------------------------------------------------#
		# A.		   --SUPERBREAKER--			#
		#-------------------------------------------------------#
on right click:
	if {@Mining Ability} is true:
		player is holding a pickaxe:
			if clicked block is iron block or gold block:
				stop
			else:
				if {skmmo::%{skmmoVar.%player%}%::mining::skills::superbreaker::toggle} is false:
					set {_difference} to difference between {skmmo::%{skmmoVar.%player%}%::mining::skills::superbreaker::cooldowner} and now
					if {_difference} is less than {@cooldown.superbreaker}:
						set {_difference} to difference between {@cooldown.superbreaker} and {_difference}
						send "&cYou are too tired to use this ability. &e(%{_difference}%)"
						if {skmmo::%{skmmoVar.%player%}%::mining::skills::superbreaker::cooldownmsg} is false:
							set {skmmo::%{skmmoVar.%player%}%::mining::skills::superbreaker::cooldownmsg} to true
							wait {_difference}
							send "&aYour &eSuper Breaker &ais Refreshed!"
							set {skmmo::%{skmmoVar.%player%}%::mining::skills::superbreaker::cooldownmsg} to false
					else:
						send "&a** YOU RAISE YOUR PICKAXE **"
						set {skmmo::%{skmmoVar.%player%}%::mining::skills::superbreaker::toggle} to true
						wait 1 seconds
						if {skmmo::%{skmmoVar.%player%}%::mining::skills::superbreaker::use} is false:
							send "&7** YOU LOWER YOUR PICKAXE **"
							set {skmmo::%{skmmoVar.%player%}%::mining::skills::superbreaker::toggle} to false

on left click:
	if {@Mining Ability} is true:
		player is holding a pickaxe:
			if {skmmo::%{skmmoVar.%player%}%::mining::skills::superbreaker::toggle} is true:
				set {skmmo::%{skmmoVar.%player%}%::mining::skills::superbreaker::toggle} to false
				set {skmmo::%{skmmoVar.%player%}%::mining::skills::superbreaker::use} to true
				send "&a&l*** Super-Breaker Activated ***"
				set {skmmo::%{skmmoVar.%player%}%::mining::skills::superbreaker::cooldowner} to now
				wait {skmmo::%{skmmoVar.%player%}%::mining::skills::superbreaker::durability}
				send "&7&l*** Super-Breaker Deactivated ***"
				set {skmmo::%{skmmoVar.%player%}%::mining::skills::superbreaker::use} to false

on left click:
	if {@Mining Ability} is true:
		player is holding a pickaxe:
			if {skmmo::%{skmmoVar.%player%}%::mining::skills::superbreaker::use} is true:
				if clicked block is stone or redstone ore or gold ore or lapis ore or iron ore or diamond ore or emerald ore or glowstone or obsidian or end stone or nether quartz ore or netherrack or mossy cobblestone or sandstone:
					if {skmmo::blocklog::%location of event-block%} isn't set:
						if clicked block is stone:
							drop 1 of cobblestone at clicked block
							add {@mining.stone.reward} to {skmmo::%{skmmoVar.%player%}%::mining::xp}
						if clicked block is netherrack:
							drop 1 of netherrack at clicked block
							add {@mining.netherrack.reward} to {skmmo::%{skmmoVar.%player%}%::mining::xp}
						if clicked block is sandstone:
							drop 1 of sandstone at clicked block
							add {@mining.sandstone.reward} to {skmmo::%{skmmoVar.%player%}%::mining::xp}
						if clicked block is mossy cobblestone:
							drop 1 of mossy cobblestone at clicked block
							add {@mining.mossycobblestone.reward} to {skmmo::%{skmmoVar.%player%}%::mining::xp}
						if clicked block is gold ore:
							drop 1 of gold ore at clicked block
							add {@mining.gold.reward} to {skmmo::%{skmmoVar.%player%}%::mining::xp}
						if clicked block is glowstone:
							drop 4 of glowstone dust at clicked block
							add {@mining.glowstone.reward} to {skmmo::%{skmmoVar.%player%}%::mining::xp}
						if clicked block is lapis ore:
							drop 4 of lapis at clicked block
							add {@mining.lapis.reward} to {skmmo::%{skmmoVar.%player%}%::mining::xp}
						if clicked block is redstone ore:
							drop 5 of redstone dust at clicked block
							add {@mining.redstone.reward} to {skmmo::%{skmmoVar.%player%}%::mining::xp}
						if clicked block is emerald ore:
							drop 1 of emerald at clicked block
							add {@mining.emerald.reward} to {skmmo::%{skmmoVar.%player%}%::mining::xp}
						if clicked block is diamond ore:
							drop 1 of diamond at clicked block
							add {@mining.diamond.reward} to {skmmo::%{skmmoVar.%player%}%::mining::xp}
						if clicked block is iron ore:
							drop 1 of iron ore at clicked block
							add {@mining.iron.reward} to {skmmo::%{skmmoVar.%player%}%::mining::xp}
						if clicked block is obsidian:
							if player is holding a diamond pickaxe:
								drop 1 of obsidian at clicked block
								add {@mining.obsidian.reward} to {skmmo::%{skmmoVar.%player%}%::mining::xp}
							else:
								stop
						if clicked block is end stone:
							drop 1 of end stone at clicked block
							add {@mining.endstone.reward} to {skmmo::%{skmmoVar.%player%}%::mining::xp}
						if clicked block is nether quartz ore:
							drop 4 of nether quartz at clicked block
							add {@mining.quartz.reward} to {skmmo::%{skmmoVar.%player%}%::mining::xp}
						set clicked block to air
						make console execute command "skmmo check %player% Mining"
