							send "&aYour &eGiga Drill &ais Refreshed!"
							set {skmmo::%{skmmoVar.%player%}%::excavation::skills::gigadrill::cooldownmsg} to false
					else:
						send "&a** YOU RAISE YOUR SHOVEL **"
						set {skmmo::%{skmmoVar.%player%}%::excavation::skills::gigadrill::toggle} to true
						wait 1 seconds
						if {skmmo::%{skmmoVar.%player%}%::excavation::skills::gigadrill::use} is false:
							send "&7** YOU LOWER YOUR SHOVEL **"
							set {skmmo::%{skmmoVar.%player%}%::excavation::skills::gigadrill::toggle} to false
